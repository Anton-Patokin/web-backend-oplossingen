RewriteEngine on 

#RewriteRule ^/?test.html$ test.php [L]
#RewriteRule ^/test.php/?([a-zA-Z_]+)/([a-zA-Z_]+)/([a-zA-Z_]+)$ test.php?country=$1&state=$2&city=$3 [L]

