/*---------------------------------------------------------------*/

.container:after {
 clear: both;
} 
.container:before, .container:after {
 display: table;
 line-height: 0;
 content: "";
}

/*------------------------------------------------------------*/
.clearfix {
 *zoom: 1;
} 
.clearfix:before, .clearfix:after {
 display: table;
 line-height: 0;
 content: "";
} 
.clearfix:after {
 clear: both;
}


  